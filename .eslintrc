{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:flowtype/recommended"
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": [2, { "ignoreRestSiblings": true }]
  },
  "plugins": ["flowtype"]
}
